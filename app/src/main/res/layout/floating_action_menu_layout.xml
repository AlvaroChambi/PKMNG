<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <es.developer.achambi.pkmng.core.ui.FloatingTextButton
        android:id="@+id/configuration_floating_save_button"
        android:layout_width="86dp"
        android:layout_height="86dp"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginLeft="0dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toLeftOf="@+id/configuration_floating_save_button2"
        android:layout_marginRight="0dp"
        app:layout_constraintRight_toRightOf="@+id/configuration_floating_save_button2"
        android:layout_marginBottom="8dp"
        app:layout_constraintBottom_toTopOf="@+id/configuration_floating_save_button2">
    </es.developer.achambi.pkmng.core.ui.FloatingTextButton>

    <es.developer.achambi.pkmng.core.ui.FloatingTextButton
        android:id="@+id/configuration_floating_save_button1"
        android:layout_width="86dp"
        android:layout_height="86dp"
        android:layout_gravity="center_horizontal|bottom"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toTopOf="@+id/configuration_floating_save_button"
        app:layout_constraintBottom_toBottomOf="@+id/configuration_floating_save_button"
        android:layout_marginBottom="0dp"
        app:layout_constraintRight_toLeftOf="@+id/configuration_floating_save_button"
        android:layout_marginRight="8dp">
    </es.developer.achambi.pkmng.core.ui.FloatingTextButton>

    <es.developer.achambi.pkmng.core.ui.FloatingTextButton
        android:id="@+id/configuration_floating_save_button2"
        android:layout_width="86dp"
        android:layout_height="86dp"
        android:layout_gravity="center_horizontal|bottom"
        app:layout_constraintBottom_toBottomOf="parent"
        android:layout_marginBottom="8dp"
        android:layout_marginRight="8dp"
        app:layout_constraintRight_toRightOf="parent"
        android:layout_marginLeft="8dp"
        app:layout_constraintLeft_toLeftOf="parent"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp">
    </es.developer.achambi.pkmng.core.ui.FloatingTextButton>

    <es.developer.achambi.pkmng.core.ui.FloatingTextButton
        android:id="@+id/configuration_floating_save_button3"
        android:layout_width="86dp"
        android:layout_height="86dp"
        android:layout_gravity="center_horizontal|bottom"
        app:layout_constraintBottom_toBottomOf="@+id/configuration_floating_save_button"
        android:layout_marginBottom="0dp"
        android:layout_marginLeft="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginEnd="8dp"
        app:layout_constraintLeft_toRightOf="@+id/configuration_floating_save_button"
        app:layout_constraintTop_toTopOf="@+id/configuration_floating_save_button"
        android:layout_marginTop="0dp">
    </es.developer.achambi.pkmng.core.ui.FloatingTextButton>


</android.support.constraint.ConstraintLayout>